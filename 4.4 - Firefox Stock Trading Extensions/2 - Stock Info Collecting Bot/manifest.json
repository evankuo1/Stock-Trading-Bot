{

  "manifest_version": 2,
  "name": "Info Collecting Bot",
  "version": "1.0",

  "description": "Prints out stock price so that we can save it and use it for testing.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://robinhood.com/stocks/*"],
      "js": ["infoRecorder.js"]
    }
  ],

  "permissions": ["storage"]

}